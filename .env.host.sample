# Ollama Model Settings (must match models available on your host Ollama)
MODEL=llama2:latest
EMBEDDING_MODEL=all-minilm:l6-v2

# Document chunking settings
ENABLE_CHUNKING=true
MAX_CHUNK_SIZE=1000
MIN_CHUNK_SIZE=200
CHUNK_OVERLAP=100

# Web Search Integration
SERPER_API_KEY=your_serper_dev_api_key_here

# Host Ollama URL - uncomment the appropriate one:
# HOST_OLLAMA=http://host.docker.internal:11434  # For Windows/macOS
# HOST_OLLAMA=http://localhost:11434             # For Linux (rarely works)